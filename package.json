{
  "name": "synapse",
  "version": "1.0.0",
  "description": "Agnostic CLI tool that automates pipeline flows via commands for web development",
  "main": "./lib/synapse.js",
  "bin": {
    "synapse": "./bin/synapse"
  },
  "scripts": {
    "dev": "cross-env DEBUG=synapse:dev NODE_ENV=dev nodemon bin/synapse",
    "test": "cross-env DEBUG=synapse:test NODE_ENV=test _mocha --w --config .mocharc.js \\\"test/unit/**/*_spec.js\\\"",
    "it": "cross-env DEBUG=synapse:integration NODE_ENV=test _mocha --config .mocharc.js \\\"test/integration/**/*_spec.js\\\"",
    "coverage": "cross-env NODE_ENV=test nyc mocha --config .mocharc.js \\\"test/unit/**/*_spec.js\\\""
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".js"
    ],
    "temp-dir": ".cache",
    "report-dir": "coverage",
    "include": [
      "lib/**"
    ],
    "cache": true,
    "sourceMap": true,
    "all": true,
    "instrument": false
  },
  "files": [
    "bin",
    "lib",
    "@types",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kuakman/synapse.git"
  },
  "keywords": [
    "cli",
    "pipeline",
    "development-tools"
  ],
  "types": "./@types/index.d.ts",
  "author": "Patricio Ferreira <3dimentionar@gmail.com>",
  "license": "UNLICENSED",
  "nodemonConfig": {
    "watch": [
      "./lib/**/*",
      "./test/**/*",
      "./package.json"
    ]
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "synapse",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-shorthand-properties": "^7.8.3",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "babel-plugin-istanbul": "6.0.0",
    "chai": "4.2.0",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-chai-assert-bdd": "0.0.3",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-mocha": "6.2.2",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "mocha": "7.0.1",
    "nodemon": "2.0.2",
    "nyc": "15.0.0",
    "sinon": "8.1.1",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "@oclif/color": "0.1.0",
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-help": "2.2.3",
    "babel-plugin-module-resolver": "4.0.0",
    "cli-ux": "5.4.4",
    "debug": "4.1.1",
    "extend": "3.0.2",
    "fs-extra": "8.1.0",
    "globby": "11.0.0",
    "json5": "2.1.1",
    "micro": "9.3.4",
    "promise-sequential": "1.1.1",
    "underscore": "1.9.2"
  }
}
